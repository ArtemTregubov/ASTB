	ПоставщикВыбран = Ложь;
	ПараметрПоставщик = Настройки.ПараметрыДанных.НайтиЗначениеПараметра(Новый ПараметрКомпоновкиДанных("Поставщик"));
	Если НЕ ПараметрПоставщик = Неопределено Тогда
		ВыбранныйПоставщик = ПараметрПоставщик.Значение;
		Если ВыбранныйПоставщик <> Справочники.Контрагенты.ПустаяСсылка()
			И ПараметрПоставщик.Использование = Истина Тогда
			ПоставщикВыбран = Истина;
		КонецЕсли;
	Иначе
		Возврат;
	КонецЕсли;
	
